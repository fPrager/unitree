{
    "parser": "babel-eslint",
    "root": true,
    "extends": ["xo/esnext", "xo-react", "airbnb"],
    "globals": {
     },
    "rules": {
      "indent": [2, 4],
      "babel/new-cap": 0,
      "babel/arrow-parens": 0,
      "max-len": [
        "error",
        {
          "code": 100,
          "comments": 100,
          "ignoreTrailingComments": true,
          "ignoreUrls": true,
          "ignoreRegExpLiterals": true,
          "tabWidth": 4
        }
      ],
      "arrow-body-style": 0,
      "import/no-extraneous-dependencies": 0,
      "import/no-unresolved": 0,
      "import/extensions": 0,
      "one-var": 0,
      "import/prefer-default-export": 0,
      "no-underscore-dangle": 0,
      "camelcase": 0,
      "import/no-absolute-path": 0
    }
  }
  